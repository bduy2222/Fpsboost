-- ðŸŒŸ FPS BOOST + UNLOCK FPS (Universal)
-- Made by   ChatGPT

-- âš¡ FPS BOOST
local function FPSBoost()
    -- Táº¯t hiá»‡u á»©ng thá»«a
    for _, v in pairs(game:GetDescendants()) do
        if v:IsA("ParticleEmitter") or v:IsA("Trail") then
            v.Lifetime = NumberRange.new(0)
        elseif v:IsA("Explosion") then
            v.BlastPressure = 1
            v.BlastRadius = 1
        elseif v:IsA("Fire") or v:IsA("Smoke") then
            v.Enabled = false
        elseif v:IsA("Decal") or v:IsA("Texture") then
            v:Destroy()
        elseif v:IsA("MeshPart") then
            v.Material = Enum.Material.Plastic
            v.Reflectance = 0
        elseif v:IsA("UnionOperation") or v:IsA("Part") then
            v.Material = Enum.Material.Plastic
            v.Reflectance = 0
        end
    end

    -- Giáº£m cháº¥t lÆ°á»£ng Lighting
    if game.Lighting:FindFirstChild("Atmosphere") then game.Lighting.Atmosphere:Destroy() end
    if game.Lighting:FindFirstChild("Sky") then game.Lighting.Sky:Destroy() end
    if game.Lighting:FindFirstChild("Bloom") then game.Lighting.Bloom:Destroy() end
    if game.Lighting:FindFirstChild("ColorCorrection") then game.Lighting.ColorCorrection:Destroy() end
    if game.Lighting:FindFirstChild("SunRays") then game.Lighting.SunRays:Destroy() end
    if game.Lighting:FindFirstChild("DepthOfField") then game.Lighting.DepthOfField:Destroy() end
end

-- ðŸ”“ UNLOCK FPS (há»— trá»£ executor cÃ³ setfpscap)
local function UnlockFPS()
    if setfpscap then
        setfpscap(999) -- má»Ÿ khÃ³a FPS max
    elseif set_fps_cap then
        set_fps_cap(999)
    else
        warn("Executor khÃ´ng há»— trá»£ unlock FPS trá»±c tiáº¿p!")
    end
end

-- ðŸš€ Cháº¡y tá»‘i Æ°u
FPSBoost()
UnlockFPS()
print("âœ… FPS Boost + Unlock FPS Ä‘Ã£ báº­t!")
